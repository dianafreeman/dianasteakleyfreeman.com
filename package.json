  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "build:component": "npx create-component-app --config ./scripts/ccarc.config.js -t DianaBookES7",
    "build:storybook": "NODE_ENV=storybook build-storybook",
    "cy:run": "NODE_ENV=testing cypress run --config-file=./test/cypress.json",
    "cy:open": "NODE_ENV=testing cypress open --config-file=./test/cypress.json",
    "dev": "yarn clean && NODE_ENV=development gatsby develop -o",
    "clean": "gatsby clean",
    "format": "npx prettier --write 'src/**/*.{js,jsx,json}'",
    "start": "gatsby serve",
    "storybook": "NODE_ENV=storybook start-storybook --quiet -p 6006",
    "test": "run-p test:unit test:format",
    "test:dev": "NODE_ENV=testing jest --config ./tests/jest.config.js --watchAll",
    "test:unit": "NODE_ENV=testing jest --config ./tests/jest.config.js--collectCoverage",
    "test:format": "NODE_ENV=testing npx prettier --check 'src/**/*.{js,jsx,json}'",
    "ci": "NODE_ENV=testing run-p start cy:run",
    "test:e2e:dev": "NODE_ENV=testing cypress open"
  },
