  "scripts": {
    "build": "gatsby build",
    "build:component": "npx create-component-app --config ./scripts/ccarc.config.js -t DianaBookES7",
    "build:storybook": "build-storybook",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "dev": "gatsby develop -o",
    "format": "npx prettier --write 'src/**/*.{js,jsx,json}'",
    "start": "gatsby serve",
    "start:storybook": "start-storybook --quiet -p 6006",
    "test": "run-p test:unit test:format",
    "test:dev": "jest --config=src/config/jest.config.js --watchAll",
    "test:storybook": "start-storybook --smoke-test ",
    "test:unit": "jest ./src/*.spec.{js, jsx} --collectCoverage",
    "test:format": "npx prettier --check 'src/**/*.{js,jsx,json}'",
    "test:e2e:ci": "start-server-and-test start http://localhost:9000 cy:run",
    "test:e2e:dev": "cypress open"
  },
